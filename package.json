{
  "name": "svelte-template-multi",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "nodemon src/server/index.ts",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "pages:new": "ts-node ./scripts/newPage.ts",
    "pages:delete": "ts-node ./scripts/deletePage.ts",
    "pages:switch": "ts-node ./scripts/changePage.ts"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.0",
    "@types/express": "^4.17.13",
    "node-sass": "^6.0.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "express": "^4.17.1",
    "sirv-cli": "^1.0.0"
  }
}
